<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IGamePlay" />
                <binding name="WSDualHttpBinding_IGamePlay1" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8888/GameService/" binding="wsDualHttpBinding"
                bindingConfiguration="WSDualHttpBinding_IGamePlay" contract="GameServiceReference.IGamePlay"
                name="WSDualHttpBinding_IGamePlay">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8888/GameService/" binding="wsDualHttpBinding"
                bindingConfiguration="WSDualHttpBinding_IGamePlay1" contract="GameServiceReference.IGamePlay"
                name="WSDualHttpBinding_IGamePlay1">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>
